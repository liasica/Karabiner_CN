<?xml version="1.0"?>
<root>
  <block>
    <modifier_not>
      ModifierFlag::COMMAND_L,
      ModifierFlag::COMMAND_R,
      ModifierFlag::CONTROL_L,
      ModifierFlag::CONTROL_R,
    </modifier_not>
    <include path="../commons/wrap_keys/alphabet.xml" />
    <include path="../commons/wrap_keys/number.xml" />
    <include path="../commons/wrap_keys/symbol.xml" />
    <include path="../commons/wrap_keys/keypad.xml" />
    <include path="../commons/wrap_keys/international.xml" />

    <!-- pickup from special.xml -->
    <autogen>__KeyToKey__ KeyCode::ENTER,           {{ BEFORE }}, KeyCode::ENTER,           {{ AFTER }}</autogen>
    <autogen>__KeyToKey__ KeyCode::ENTER_POWERBOOK, {{ BEFORE }}, KeyCode::ENTER_POWERBOOK, {{ AFTER }}</autogen>
    <autogen>__KeyToKey__ KeyCode::RETURN,          {{ BEFORE }}, KeyCode::RETURN,          {{ AFTER }}</autogen>
    <autogen>__KeyToKey__ KeyCode::SPACE,           {{ BEFORE }}, KeyCode::SPACE,           {{ AFTER }}</autogen>
    <autogen>__KeyToKey__ KeyCode::TAB,             {{ BEFORE }}, KeyCode::TAB,             {{ AFTER }}</autogen>
  </block>
</root>
